{
  "manifest_version": 2,

  "name": "Scrap해요",
  "description": "필요한 컨텐츠를 저장해보세요",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://localhost/",
    "http://localhost/*/",
    "http://*/"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
